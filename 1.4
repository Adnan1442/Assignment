#include<stdio.h>
int main(){
    int i;
    float f;
    printf("Enter a integer number: \n");
    scanf("%d",&i);
    printf("Enter a floating number: \n");
    scanf("%f",&f);
    printf("The integer number is %d and the floating number is %f",i,f);
}



